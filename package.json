{
  "name": "react-redux-typescript-starter-kit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "webpack-dashboard -- webpack serve --hot --color --config ./webpack.config.dev.js",
    "start:prod": "npm run clean-dist && webpack --config ./webpack.config.prod.js --color",
    "start:test": "npm run clean-dist-stable && webpack --config ./webpack.config.test.js",
    "eslint": "./node_modules/.bin/eslint ./src/**/*.{ts,tsx}",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "clean-dist-stable": "npm run remove-dist-stable && mkdir dist-stable",
    "remove-dist": "rimraf ./dist",
    "remove-dist-stable": "rimraf ./dist-stable",
    "generate": "babel-node tools/generateTemplate/generate.js",
    "ungenerate": "babel-node tools/generateTemplate/ungenerate.js",
    "test": "jest --no-cache --runInBand --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@redux-dynostore/core": "^3.1.0",
    "@redux-dynostore/react-redux": "^3.1.0",
    "@redux-dynostore/react-redux-subspace": "^3.1.0",
    "@redux-dynostore/redux-subspace": "^3.1.0",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.15",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.5",
    "axios": "^0.21.0",
    "connected-react-router": "^6.8.0",
    "history": "^5.0.0",
    "jwt-decode": "^3.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.6.3",
    "typesafe-actions": "^5.1.0",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-numeric-separator": "^7.12.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.8.0",
    "babel-loader": "^8.2.1",
    "bundle-loader": "^0.5.6",
    "cache-loader": "^4.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.13.0",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^6.2.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "immer": "^7.0.14",
    "jest": "^26.6.3",
    "json-loader": "^0.5.7",
    "lint-staged": "^10.5.1",
    "mini-css-extract-plugin": "^1.3.1",
    "node-sass": "^5.0.0",
    "postcss-loader": "^4.0.4",
    "prettier": "2.1.2",
    "sass-loader": "^10.1.0",
    "speed-measure-webpack-plugin": "^1.3.3",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "ts-loader": "^8.0.11",
    "url-loader": "^4.1.1",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dashboard": "^3.2.1",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": [
        "npm run eslint",
        "prettier --write",
        "git add"
      ],
      "*.json": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/*.min.js",
      "node_modules"
    ]
  },
  "browserslist": [
    "defaults"
  ]
}
