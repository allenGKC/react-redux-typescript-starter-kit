{
  "name": "react-redux-ts-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "webpack-dev-server --hot --color --config ./webpack.config.dev.js",
    "start:prod": "npm run clean-dist && webpack --config ./webpack.config.prod.js",
    "start:stable": "npm run clean-dist-stable && webpack --config ./webpack.config.stable.js",
    "eslint": "./node_modules/.bin/eslint ./src/**/*.{ts,tsx}",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "clean-dist-stable": "npm run remove-dist-stable && mkdir dist-stable",
    "remove-dist": "rimraf ./dist",
    "remove-dist-stable": "rimraf ./dist-stable",
    "generate": "babel-node tools/generateTemplate/generate.js",
    "ungenerate": "babel-node tools/generateTemplate/ungenerate.js",
    "test": "jest",
    "eject": "react-scripts-ts eject"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@types/history": "^4.7.3",
    "@types/jest": "^25.2.2",
    "@types/node": "^12.7.8",
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.1",
    "@types/react-redux": "^7.1.4",
    "@types/react-router-dom": "^5.1.5",
    "axios": "^0.19.2",
    "connected-react-router": "^6.5.2",
    "history": "^4.10.1",
    "react": "^16.10.0",
    "react-dom": "^16.10.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.2.0",
    "react-scripts-ts": "^4.0.8",
    "redux": "^4.0.4",
    "redux-observable": "^1.2.0",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.5.3",
    "typesafe-actions": "^5.1.0",
    "typescript": "^3.9.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-proposal-private-methods": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/webpack-env": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.2.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "immer": "^6.0.5",
    "jest": "^24.9.0",
    "json-loader": "^0.5.7",
    "lint-staged": "^9.4.3",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "prettier": "1.19.1",
    "sass-loader": "^8.0.0",
    "speed-measure-webpack-plugin": "^1.3.1",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.1.2",
    "url-loader": "^2.1.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": [
        "npm run eslint",
        "prettier --write",
        "git add"
      ],
      "*.json": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/*.min.js",
      "node_modules"
    ]
  }
}
